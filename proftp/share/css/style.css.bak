        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3f4c6b', endColorstr='#606c88',GradientType=0 ); /* IE6-9 */
	margin:0;
	padding:0;
}

td.tablink{
        color:#FFFFFF;
        padding-top:2px;
        padding-bottom:2px;
        padding-left:6px;
        padding-right:6px;
        border-right:1px solid gray;
}

td.tablink:hover{
	cursor:pointer;
	background: -moz-linear-gradient(top, rgba(0,0,0,0) 0%, rgba(0,0,0,0.35) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0.35))); /* Chrome,Safari4+ */
