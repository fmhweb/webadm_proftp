td.menuright{
	color:#FFFFFF;
	padding-top:5px;
	padding-bottom:5px;
	padding-left:8px;
	padding-right:8px;
	border-right:1px solid gray;
}

table.result{
	table-layout: fixed;
	height:93%;
	left:0px;
	width:98%;
	padding:0;
	margin-top:47px;
}

td.resultleft{
	background-color:#FFFFFF;
	width:100%;
	vertical-align:top;
	border-top:1px solid #D8D8D8;
	border-left:1px solid #D8D8D8;
	border-right:2px solid #BDBDBD;
	border-bottom:2px solid #BDBDBD;
	box-shadow: 2px 2px 1px #000000;
}

td.resultright{
	background-color:#FFFFFF;
	width:0%;
	display:none;
	visibility:hidden;
	vertical-align:top;
	overflow:auto;
	border-top:1px solid #D8D8D8;
	border-left:1px solid #D8D8D8;
	border-right:2px solid #BDBDBD;
	border-bottom:2px solid #BDBDBD;
	box-shadow: 2px 2px 1px #000000;
}

#debug{
	opacity:0.7;
	position:fixed;
	margin-left:50%;
	width:50%;
	z-index:11;
	background-color:#FFFFFF;
}

div.title{
	text-align:center;
	font-size:14px;
        padding-top:4px;
        padding-bottom:4px;
	width:100%;
	color:#FFFFFF;
	margin-bottom:1px;
	background: rgb(63,76,107); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(63,76,107,1) 0%, rgba(96,108,136,1) 100%); /* FF3.6+ */
