div.block{
	background-color:#FFFFFF;
	margin-bottom:5px;
	margin:20px;
	border-top:1px solid #D8D8D8;
	border-left:1px solid #D8D8D8;
	border-right:1px solid #BDBDBD;
	border-bottom:1px solid #BDBDBD;
	box-shadow: 2px 2px 1px #888888;
	overflow:hidden;
}

div.error{
	color:red;
	z-index:50;
}

span.infosmall{
	font-size:11px;
	color:gray;
}

ul.folder{
	margin-left: 0.8em;
	padding-left: 0.8em;
}

li.folder{
	list-style-type: none;
}

